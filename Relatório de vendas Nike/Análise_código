    "#REVENUE PER MONTH\n",
    "dataset['Month_Year'] = dataset['Invoice Date'].dt.to_period('M')\n",
    "sales_month = dataset.groupby(\"Month_Year\")[\"Total Sales\"].sum().sort_index()\n",
    "\n",
    "plt.figure(figsize=(12, 6))\n",
    "plt.plot(sales_month.index.astype(str), sales_month.values, \n",
    "         color='blue', linestyle='-', linewidth=2, marker='o')\n",
    "plt.title(\"Receita ao longo do tempo\", fontsize=16)\n",
    "plt.xlabel(\"Mês-Ano\", fontsize=14)\n",
    "plt.ylabel(\"Receita total (USD)\", fontsize=14)\n",
