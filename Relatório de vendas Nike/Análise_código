    "#REVENUE/REGION\n",
    "revenue_region = dataset.groupby(\"Region\")[\"Total Sales\"].sum().sort_values()\n",
    "plt.figure(figsize=(8, 5))\n",
    "plt.bar(revenue_region.index, revenue_region.values, color='skyblue')\n",
    "plt.title(\"Receita por Região\", fontsize=14)\n",
    "plt.xlabel(\"Região\", fontsize=12)\n",
    "plt.ylabel(\"Receita Total (USD)\", fontsize=12)\n",
    "plt.xticks(rotation=45)\n",
    "plt.grid(axis='y', linestyle='--', alpha=0.7)\n",
    "plt.tight_layout()\n",
    "plt.show()\n"
   ]
