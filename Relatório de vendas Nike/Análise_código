    "#REVENUE/CATEGORY (Most produtcs)\n",
    "dataset['Product_Category'] = dataset['Product'].apply(lambda x: x.split(\"'s\")[0] if \"'s\" in x else x)\n",
    "revenue_product = dataset.groupby(\"Product_Category\")[\"Total Sales\"].sum().sort_values()\n",
    "plt.figure(figsize=(8, 5))\n",
    "plt.bar(revenue_product.index, revenue_product.values, color='lightgreen')\n",
    "plt.title(\"Receita por Categoria de Produto\", fontsize=14)\n",
    "plt.xlabel(\"Categoria\", fontsize=12)\n",
    "plt.ylabel(\"Receita Total (USD)\", fontsize=12)\n",
    "plt.xticks(rotation=45)\n",
    "plt.grid(axis='y', linestyle='--', alpha=0.7)\n",
    "plt.tight_layout()\n",
